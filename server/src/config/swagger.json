    },
    "/api/v1/users/signup": {
      "post": {
        "summary": "Cadastra um novo usuário",
        "tags": ["Users"],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/NewUser"
              }
            }
          }
        },
        "responses": {
          "500": {
            "description": "Internal Server Error"
          },
          "409": {
            "description": "Conflict",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "allOf": [
                    {
                      "properties": {
                        "username conflict": {
                          "type": "object",
                          "properties": {
                            "error": {
                              "type": "string",
                              "example": "Este usuário já foi cadastrado anteriormente."
                            }
                          }
                        }
                      }
                    },
                    {
                      "properties": {
                        "email conflict": {
                          "type": "object",
                          "properties": {
                            "error": {
                              "example": "Este email já foi cadastrado anteriormente."
                            }
                          }
                        }
                      }
                    }
                  ]
                }
              }
            }
          },
          "400": {
            "description": "Dados inválidos"
          },
          "201": {
            "description": "Conta de usuário criada",
            "content": {
              "application/json": {
                "schema": {
                  "properties": {
                    "info": {
                      "example": "Conta criada com sucesso."
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
