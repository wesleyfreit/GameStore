{
  "openapi": "3.0.0",
  "info": {
    "title": "API REST Express de um gerenciador de uma loja de Jogos.",
    "description": "Esta é uma aplicação do tipo API REST feita com Express.",
    "contact": {
      "name": "Projeto GameStore",
      "url": "https://github.com/ifpb-cz-ads/pw1-2023-1-projeto-gamestore"
    },
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "http://localhost:8080",
      "description": "Servidor de desenvolvimento"
    },
    {
      "url": "https://game-store-gvng.onrender.com/",
      "description": "Servidor de produção"
    }
  ],
  "apis": ["../routes/api/*.js"],
  "paths": {
    "/api/v1/index": {
      "get": {
        "summary": "Recupera a lista de jogos",
        "tags": ["Games"],
        "responses": {
          "500": {
            "description": "Internal Server Error"
          },
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/GetGame"
                }
              }
            }
          }
        }
      }
    },
    "/api/v1/games": {
      "get": {
        "summary": "Recupera a lista de jogos",
        "description": "Necessita de autenticação de admin",
        "tags": ["Games"],
        "security": [
          {
            "Auth-Token": []
          }
        ],
        "responses": {
          "500": {
            "description": "Internal Server Error"
          },
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/GetGameAdm"
                }
              }
            }
          }
        }
      },
      "post": {
        "summary": "Cria um novo jogo",
        "description": "Necessita de autenticação de admin",
        "tags": ["Games"],
        "security": [
          {
            "Auth-Token": []
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "multipart/form-data": {
              "schema": {
                "$ref": "#/components/schemas/NewGame"
              }
            }
          }
        },
        "responses": {
          "500": {
            "description": "Internal Server Error"
          },
          "409": {
            "description": "Conflict",
            "content": {
              "application/json": {
                "schema": {
                  "properties": {
                    "error": {
                      "example": "O título que está tentando cadastrar já existe, por favor tente outro título diferente."
                    }
                  }
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized",
            "content": {
              "application/json": {
                "schema": {
                  "properties": {
                    "error": {
                      "example": "O formato dos dados atuais não parece estar correto."
                    }
                  }
                }
              }
            }
          },
          "201": {
            "description": "Created",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/GameId"
                }
              }
            }
          }
        }
      }
    },
    "/api/v1/games/{id}": {
      "put": {
        "summary": "Atualiza um jogo",
        "description": "Necessita de autenticação de admin",
        "tags": ["Games"],
        "security": [
          {
            "Auth-Token": []
          }
        ],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "description": "ID do jogo a ser recuperado para editar",
            "required": true,
            "schema": {
              "type": "string",
              "example": "fe2928cc-92z3-4c11-b7bd-9fe53sd0e87f"
            }
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "multipart/form-data": {
              "schema": {
                "$ref": "#/components/schemas/EditGame"
              }
            }
          }
        },
        "responses": {
          "500": {
            "description": "Internal Server Error"
          },
          "409": {
            "description": "Conflict",
            "content": {
              "application/json": {
                "schema": {
                  "properties": {
                    "error": {
                      "example": "O novo nome que você quer salvar para o título do jogo atual já existe, tente outro nome."
                    }
                  }
                }
              }
            }
          },
          "404": {
            "description": "Content not found",
            "content": {
              "application/json": {
                "schema": {
                  "properties": {
                    "error": {
                      "example": "O jogo que você tentou acessar para atualizar provalvelmente não existe."
                    }
                  }
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized",
            "content": {
              "application/json": {
                "schema": {
                  "properties": {
                    "error": {
                      "example": "Ocorreu um erro ao validar os dados, verifique se preencheu tudo corretamente e tente novamente."
                    }
                  }
                }
              }
            }
          },

          "204": {
            "description": "O jogo foi atualizado."
          }
        }
      },
      "delete": {
        "summary": "Remove um jogo",
        "description": "Necessita de autenticação de admin",
        "tags": ["Games"],
        "security": [
          {
            "Auth-Token": []
          }
        ],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "description": "ID do jogo a ser recuperado para remover",
            "required": true,
            "schema": {
              "type": "string",
              "example": "fe2928cc-92z3-4c11-b7bd-9fe53sd0e87f"
            }
          }
        ],
        "responses": {
          "500": {
            "description": "Internal Server Error"
          },
          "404": {
            "description": "Content not found",
            "content": {
              "application/json": {
                "schema": {
                  "properties": {
                    "error": {
                      "example": "Ocorreu um erro ao validar os dados."
                    }
                  }
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized",
            "content": {
              "application/json": {
                "schema": {
                  "properties": {
                    "error": {
                      "example": "Ocorreu um erro ao validar os dados."
                    }
                  }
                }
              }
            }
          },
          "204": {
            "description": "O jogo foi removido."
          }
        }
      }
    },
    "/api/v1/games/{slug}": {
      "get": {
        "summary": "Recupera a busca por slug de um jogo",
        "tags": ["Games"],
        "parameters": [
          {
            "name": "slug",
            "in": "path",
            "description": "Slug do jogo",
